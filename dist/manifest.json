{
  "name": "searchrome-popup",
  "description": "You can fuzzy search tabs, history and bookmarks",
  "version": "0.0.1",
  "manifest_version": 2,
  "permissions": [
    "activeTab",
    "tabs",
    "history",
    "bookmarks"
  ],
  "background": {
    "scripts": ["js/background.js"],
    "persistent": false
  },
  "page_action": {
    "default_popup": "popup.html"
  }
}